//intializing variables
let num1 = 20;
let num2 = 10;
//performing arithmetic operations on initialized variables
let sum = num1 + num2;
let difference = num1 - num2;
let product = num1 * num2;
let divide = num1 / num2;
console.log("Sum of the given two numbers = "+sum);
console.log("Difference of the given two numbers = "+difference);
console.log("Product of the given two numbers = "+product);
console.log("Division of the given two numbers = "+divide);